ish\">\r\n                <Navbar />\r\n                <ReportFinish />\r\n                <Footer/>\r\n              </Route>\r\n              {/* <Route path=\"/report/:report/3\">\r\n                <Navbar />\r\n                <Report3 />\r\n              </Route>\r\n              <Route path=\"/report/:report\">\r\n                <Navbar />\r\n                <Report1 />\r\n              </Route> */}\r\n              <Route path=\"/admin\">\r\n                <Navbar />\r\n                <AdminReport />\r\n                <Footer/>\r\n              </Route>\r\n              <Route path=\"/c\">\r\n                <Navbar />\r\n                <CasesOptionPage />\r\n                <Footer/>\r\n              </Route>\r\n              <Route path=\"/tax-and-credibility\">\r\n                <Navbar />\r\n                <TaxCredibilityPage />\r\n                <Footer/>\r\n              </Route>\r\n              <Route exact path=\"/\">\r\n                <Navbar/>\r\n                <HomePage/>\r\n                <Footer/>\r\n              </Route>\r\n              <Route path='/sign-in'>\r\n                <LoginPage />\r\n              </Route>\r\n              <Route path='/sign-up'>\r\n                <RegisterPage />\r\n              </Route>\r\n            </Switch>\r\n          </Router>\r\n        </ApolloProvider>\r\n      </div>\r\n    </>\r\n  );\r\n}\r\n\r\nexport default App;\r\n","E:\\h8\\P3\\final project\\inspect-corrupt-app\\client\\src\\reportWebVitals.js",[],"E:\\h8\\P3\\final project\\inspect-corrupt-app\\client\\src\\config\\index.js",[],"E:\\h8\\P3\\final project\\inspect-corrupt-app\\client\\src\\components\\Footer\\index.jsx",["105"],"import './style.css';\r\n\r\nexport default () => {\r\n  return (\r\n    <footer className=\"footer\">\r\n      <div className=\"uk-container\">\r\n        <div className=\"uk-flex uk-flex-between\">\r\n          <div className=\"left-side\">\r\n            <h5>All Right Reserved | Copyright {new Date().