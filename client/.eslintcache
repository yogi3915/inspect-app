erMail = localStorage.getItem('email')\r\n\r\n  const [input, setInput] = useState({\r\n    case: report.split('-').join(' '),\r\n    entity: '',\r\n    province: '',\r\n    city: '',\r\n    dateHappened: '',\r\n    description: '',\r\n    isDocumentProvided: '',\r\n    involvedPerson: '',\r\n    personRole: '',\r\n    isReported: '',\r\n    isKeepInTouch: '',\r\n    aboutInspectApp: '',\r\n    UserEmail: receiverMail\r\n  })\r\n  const [location, setLocation] = useState({\r\n    province: [],\r\n    city: []\r\n  })\r\n  const [provId, setProvId] = useState('')\r\n\r\n  useEffect(() => {\r\n    fetch('https://ibnux.github.io/data-indonesia/propinsi.json')\r\n      .then(res => res.json())\r\n      .then(prov => setLocation({\r\n        ...location,\r\n        province: prov\r\n      }))\r\n      .catch(err => err)\r\n  }, [])\r\n\r\n  function getCity() {\r\n    fetch(`https://ibnux.github.io/data-indonesia/kabupaten/${provId}.json`)\r\n      .then(res => res.json())\r\n      .then(city => setLocation({\r\n        ...location,\r\n        city: city\r\n      }))\r\n      .catch(err => err)\r\n  }\r\n\r\n  function handleChange(e) {\r\n    const { name, value } = e.target\r\n    setInput({\r\n      ...input,\r\n      [name]: name === 'province' ? value.split(',')[0] : value\r\n    })\r\n    if (name === \"province\") {\r\n      setProvId(value.split(',')[1])\r\n    }\r\n  }\r\n\r\n  const onSubmit = (e) => {\r\n    e.preventDefault();\r\n    if (input.isKeepInTouch === \"true\") {\r\n      sendMail({\r\n        variables: {\r\n          payload: {\r\n            toReceiver: receiverMail,\r\n            subjectEmail: 'Report Submission',\r\n            message: `\r\n            <p>Your Report has been recorded, because you choose to keep in touch with us, we'll inform you any status of your report.</p>                       \r\n            <p>\r\n              Case: ${input.case} <br/>\r\n              Entity: ${input.entity} <br/>\r\n              Date Happened: ${input.dateHappened} <br/>\r\n              Location: ${input.city}, ${input.province} <br/>\r\n              Description: ${input.description} <br/>\r\n              Involved Person: ${input.involvedPerson}\r\n            </p>\r\n            `\r\n          }\r\n        }\r\n      })\r\n    }\r\n    const email = localStorage.getItem('email')\r\n    addReport({\r\n      variables: { payload: input }\r\n    })\r\n    history.push('/report-finish')\r\n  }\r\n\r\n  return (\r\n    <>\r\n      <Form onSubmit={e => onSubmit(e)} navigation>\r\n        <Page>\r\n          <Report1\r\n            handleChange={handleChange}\r\n            input={input}\r\n            setInput={setInput}\r\n            location={location}\r\n            getCity={getCity}\r\n          />\r\n        </Page>\r\n        <Page>\r\n          <Report2\r\n            handleChange={handleChange}\r\n            input={input}\r\n            setInput={setInput}\r\n          />\r\n        </Page>\r\n        <Page>\r\n          <Report3\r\n            handleChange={handleChange}\r\n            input={input}\r\n            setInput={setInput}\r\n          />\r\n        </Page>\r\n      </Form>\r\n    </>\r\n  )\r\n}","E:\\h8\\P3\\final project\\inspect-corrupt-app\\client\\src\\pages\\index.js",[],"E:\\h8\\P3\\final project\\inspect-corrupt-app\\client\\src\\components\\index.js",[],"E:\\h8\\P3\\final project\\inspect-corrupt-app\\client\\src\\components\\Forms\\report1.jsx",[],"E:\\h8\\P3\\final project\\inspect-corrupt-app\\client\\src\\components\\Forms\\report2.jsx",["109"],"import React from 'react';\r\nimport './reportStep2.css';\r\n\r\nexport default function Report2(props) {\r\n\r\n  const { handleChange, input, setInput } = props\r\n  return (\r\n    <div className=\"uk-container\">\r\n      <div className=\"uk-flex report-step-2\">\r\n        <div className=\"side-left\">\r\n          <h3>PLEASE DESCRIBE THE INCIDENT WITH AS MUCH DETAIL AS POSSIBLE</h3>\r\n          <div className=\"q-1-sect\">\r\n 